<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

    <!-- 스네이크 케이스(SNAKE_CASE) → 카멜 케이스(CamelCase) 자동 변환 -->
    <settings>
        <setting name="mapUnderscoreToCamelCase" value="true"/>
    </settings>

    <typeAliases>
        <typeAlias type="com.fastcampus.trustRide.dto.CarDto" alias="carDto"></typeAlias>
        <typeAlias type="com.fastcampus.trustRide.dto.ImageDto" alias="imageDto"></typeAlias>

    </typeAliases>


</configuration>
